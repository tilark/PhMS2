@model PHMS2.Models.ViewModel.PatientAverageCost
<div class="panel-body" id="getOutPatientRateBody">
    <table class="table table-hover table-bordered table-striped text-center">
        <tr>
            <th colspan="3" class="text-center">
                每次就诊人均药费
            </th>
        </tr>
        <tr>
            <td colspan="3" class="text-left">
                <small>时间：@ViewBag.startTime -- @ViewBag.endTime</small>
            </td>
        </tr>
        <tr>
            <td>就诊药物总费用</td>
            <td>同期就诊人次</td>
            <td>每次就诊人均药费</td>
        </tr>
        <tr>
            <td>@Decimal.Round(Model.PatientCost, 2)</td>
            <td>@Model.RegisterPerson</td>
            <td>@Model.AverageCost </td>
        </tr>
    </table>
</div>




